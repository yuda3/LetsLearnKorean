{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "image": "latest"
      },
      "cache": {
        "key": "v7-reanimated-317-patch"
      },
      "android": {
        "image": "ubuntu-22.04-jdk-17-ndk-r25b"
      }
    },
    "development-device": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "image": "latest"
      },
      "cache": {
        "key": "v7-reanimated-317-patch"
      },
      "android": {
        "image": "ubuntu-22.04-jdk-17-ndk-r25b"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "image": "latest"
      },
      "cache": {
        "key": "v7-reanimated-317-patch"
      },
      "android": {
        "buildType": "apk",
        "image": "ubuntu-22.04-jdk-17-ndk-r25b"
      }
    },
    "production": {
      "ios": {
        "simulator": false,
        "image": "latest"
      },
      "cache": {
        "key": "v7-reanimated-317-patch"
      },
      "android": {
        "buildType": "app-bundle",
        "image": "ubuntu-22.04-jdk-17-ndk-r25b"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "your-apple-id@example.com",
        "ascAppId": "your-app-store-connect-app-id",
        "appleTeamId": "your-apple-team-id"
      },
      "android": {
        "serviceAccountKeyPath": "./path/to/api-key.json",
        "track": "internal"
      }
    }
  }
}
